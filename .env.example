# Environment Configuration Template
# Copy this file to .env and customize as needed

# Logging Level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# Python settings
PYTHONUNBUFFERED=1
PYTHONIOENCODING=utf-8

# VBA Execution: DISABLED by default (security)
# VBA code runs with full Excel privileges - enable with caution
# Set to 'true' to enable the execute_vba tool
EXCELLM_ENABLE_VBA=false

# Maximum cells per write operation (hallucination prevention)
EXCELLM_MAX_CELLS=250
